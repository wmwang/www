{
 $sql="SELECT parent_id FROM ".$GLOBALS['ecs']->table('category')." WHERE cat_id = '$id'";
 $cat_id=$GLOBALS['db']->getOne($sql);
 if($cat_id)
 {
 return get_cat_parent($cat_id);
 }
 else
 {
 return $id;
 }
}
